package com.szaoto.ak10.common;


//监控卡版本	
public class Version {
	double dVersion;//版本号
	byte ucTper;//版本类型(0x01:标准APP版 0x11:标准BOOT版 0x02:PWM-APP版 0x12:PWM-BOOT版)
	boolean bBackup;//备份状态,视频通道。 1：P1口。0：P0口
	boolean bLockScreen;//锁屏
	boolean bInit;//初始化
	short nP0LinkState;//P0连接状态，1，有连接，0，无连接
	short nP1LinkState;//P1连接状态，1，有连接，0，无连接
	int nYear;
	int nMon;
	int nDay;
	int nSection;
	int nCorrectType;
	int nScanOut;
	int nScanOutSection;
	int nModuleEx;
	int nTestVersion;	//测试版本号，在日期的时间中的小时显示出来
}
