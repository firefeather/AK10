package com.szaoto.ak10.common.Display;


public class LastState {
	public LastState()
	{
		mLastBrightness = -1;
		mLastContrast = -1;
		mLastSaturation = -1;
		mLastColTempModule = -1;
		mLastchannelGroupID = -1;
		isCabinetShowing = false;//配置界面，工具状态
		isChannelShowing = false;
		isInterfaceShowing = false;
		isLableShowing = false;
	}
	int mLastBrightness;
	int mLastContrast;
	int mLastSaturation; 
	int mLastColTempModule;//色温2600k-9600k
	
	boolean isCabinetShowing;//配置界面，工具状态
	boolean isChannelShowing;
	boolean isInterfaceShowing;
	boolean isLableShowing;

	
	//int mModuleNum;//模板号1-5
	
	
	int mLastchannelGroupID;//视频源loc，DVI，Hdmi等
	public int getmLastBrightness() {
		return mLastBrightness;
	}
	public void setmLastBrightness(int mLastBrightness) {
		this.mLastBrightness = mLastBrightness;
	}
	public int getmLastContrast() {
		return mLastContrast;
	}
	public void setmLastContrast(int mLastContrast) {
		this.mLastContrast = mLastContrast;
	}
	public int getmLastSaturation() {
		return mLastSaturation;
	}
	public void setmLastSaturation(int mLastSaturation) {
		this.mLastSaturation = mLastSaturation;
	}
	public int getLastColTempModule() {
		return mLastColTempModule;
	}
	public void setLastColTempModule(int colTempModule) {
		mLastColTempModule = colTempModule;
	}
	public int getLastmchannelGroupID() {
		return mLastchannelGroupID;
	}
	public void setLastmchannelGroupID(int channelGroupID) {
		this.mLastchannelGroupID = channelGroupID;
	}
	public int getmLastColTempModule() {
		return mLastColTempModule;
	}
	public void setmLastColTempModule(int mLastColTempModule) {
		this.mLastColTempModule = mLastColTempModule;
	}
	public boolean isCabinetShowing() {
		return isCabinetShowing;
	}
	public void setCabinetShowing(boolean isCabinetShowing) {
		this.isCabinetShowing = isCabinetShowing;
	}
	public boolean isChannelShowing() {
		return isChannelShowing;
	}
	public void setChannelShowing(boolean isChannelShowing) {
		this.isChannelShowing = isChannelShowing;
	}
	public boolean isInterfaceShowing() {
		return isInterfaceShowing;
	}
	public void setInterfaceShowing(boolean isInterfaceShowing) {
		this.isInterfaceShowing = isInterfaceShowing;
	}
	public boolean isLableShowing() {
		return isLableShowing;
	}
	public void setLableShowing(boolean isLableShowing) {
		this.isLableShowing = isLableShowing;
	}
	public int getmLastchannelGroupID() {
		return mLastchannelGroupID;
	}
	public void setmLastchannelGroupID(int mLastchannelGroupID) {
		this.mLastchannelGroupID = mLastchannelGroupID;
	}
}

