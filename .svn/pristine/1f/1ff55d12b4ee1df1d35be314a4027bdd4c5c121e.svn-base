package com.szaoto.ak10.common;

public class ErrorPoint {
	short nX;					//出错LED灯在箱体内X坐标
	short nY;					//出错LED灯在箱体内Y坐标
	boolean bRedFlag;				//红灯标识 0- 正常	1-坏
	boolean bGreenFlag;			//绿灯标识
	boolean bBlueFlag;				//蓝灯标识
	ErrorPoint()
	{
		nX = 0;
		nY = 0;
		bRedFlag = false;
		bGreenFlag = false;
		bBlueFlag = false;
	}
}
