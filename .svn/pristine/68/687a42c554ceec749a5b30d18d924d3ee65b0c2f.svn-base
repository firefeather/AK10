<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginBottom="20dp"
    android:layout_marginLeft="20dp"
    android:layout_marginRight="20dp"
    android:layout_marginTop="20dp"
    android:background="@drawable/common_bg"
    tools:ignore="UselessParent,HardcodedText,DuplicateIds,TextFields,ScrollViewSize,ObsoleteLayoutParam" >

    <RelativeLayout
        android:id="@+id/cabinet_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

       
        <TextView
                android:id="@+id/text_cabinettback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="18dp"
                android:text="@string/cabinet_back_text"
                android:gravity="center_horizontal"
                android:layout_marginTop="12dp"
                android:drawableLeft="@drawable/selector_tool_back"
                android:textSize="@dimen/size25" />

        <TextView
            android:id="@+id/tv_cabinetInfo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
           android:layout_alignParentLeft="true"
            android:text="@string/tv_cabinet_info" 
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textSize="@dimen/size30"
            />

         <TextView
        android:id="@+id/tvMessageAuto"
        android:layout_width="fill_parent"
        android:layout_marginTop="4dp"
        android:layout_height="2dp"
        android:layout_below="@+id/text_cabinettback"
        android:background="@color/white" />
    </RelativeLayout>

   <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/cabinet_top" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/cabinet_top"
            android:layout_marginTop="8dp"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/RelativeLayout02"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="16dp"
                android:layout_weight="1" >

               
                <TextView
                    android:id="@+id/tv_modle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="@string/tv_model_x"
                    android:textSize="25sp" />
                <TextView
                    android:id="@+id/tvModelInfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_modle"
                    android:layout_weight="1"
                    android:text=" " />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/RelativeLayout01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                 android:layout_marginLeft="16dp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/tv_scancard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:text="@string/tv_scancard_info"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/tvScancardInfo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_scancard"
                    android:text=" " />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/layout_cabinet_mid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                 android:layout_marginLeft="16dp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/tv_monitor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_monitorcard_info"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/tvMonitorInfo2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@id/tv_monitor"
                    android:text=" " />

            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>