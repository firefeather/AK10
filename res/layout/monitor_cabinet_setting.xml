<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Layout_monitor_cabinet_setting"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/common_bg"
    android:orientation="vertical"
    tools:ignore="HardcodedText,UselessParent" >

    <LinearLayout
        android:id="@+id/absoluteLayout1"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginLeft="30dp"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="117dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView7"
                android:layout_width="603dp"
                android:layout_height="81dp"
                android:layout_marginTop="18dp"
                android:gravity="left"
                android:text="@string/monitor_cabinet_setting"
                android:textSize="40sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.05" >

            <TextView
                android:id="@+id/intervel"
                android:layout_width="300dp"
                android:gravity="left"
                android:layout_height="wrap_content"
                android:text="@string/monitorconfig_tempalarm"
                android:textSize="@dimen/size25" />

            <EditText
                android:id="@+id/editcabT"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:imeOptions="actionGo"
                android:inputType="numberDecimal"
                android:text="10"
                android:textSize="@dimen/size25" >

                <requestFocus />
            </EditText>

            <TextView
                android:id="@+id/TextView04"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:text="C"
                android:textSize="30sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.05" >

            <TextView
                android:id="@+id/autotest"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="@string/monitorconfig_humialarm"
                android:textSize="@dimen/size25" />

            <EditText
                android:id="@+id/editcabH"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:imeOptions="actionNext"
                android:inputType="numberDecimal"
                android:text="10"
                android:textSize="@dimen/size25" />

            <TextView
                android:id="@+id/TextView05"
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:text="%"
                android:textSize="30sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.18" >

            <TextView
                android:id="@+id/FrameDvi1"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="@string/monitorconfig_badlightnum_cabinet"
                android:textSize="@dimen/size25" />

            <EditText
                android:id="@+id/editcabBadnum"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:imeOptions="actionDone"
                android:inputType="numberDecimal"
                android:text="10"
                android:textSize="@dimen/size25" />

            <TextView
                android:id="@+id/TextView06"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:text="@string/numberunit"
                android:textSize="@dimen/size25" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>