<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/textMinT"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:layout_margin="8dp"
    tools:ignore="HardcodedText,ContentDescription,Deprecated,DuplicateIds,InefficientWeight,UselessParent,UseCompoundDrawables,DisableBaselineAlignment,UnknownIdInLayout" >

  
<RelativeLayout
    android:id="@+id/Layout_MonitorTitle"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:background="@drawable/common_bg" >

    <TextView
        android:id="@+id/textViewPathTitile"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:gravity="center_vertical"
        android:text="@string/Monitor_title"
        android:textSize="25sp" />

        <LinearLayout
            android:id="@+id/main_monitorback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:orientation="vertical" >

            <Button
                android:id="@+id/btn_MonitorBack"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:background="@drawable/selector_tool_back" />

            <TextView
                android:id="@+id/text_monitorback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/cabinet_back_text"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_setting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/main_monitorback"
            android:orientation="vertical" >

            <Button
                android:id="@+id/btn_Monitor_setings"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:background="@drawable/selector_settings" />

            <TextView
                android:id="@+id/text_playersetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/tool_setting_text"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/main_setting"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/MonitorHome"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:src="@drawable/selector_home" />

            <TextView
                android:id="@+id/text_playermain"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/ledconfig_main"
                android:textSize="@dimen/size15" />
        </LinearLayout>
</RelativeLayout>
    <Button
        android:id="@+id/buttonread"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/linearLayout1"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/linearLayout1"
        android:background="@drawable/button_selector"
        android:text="@string/monitor_read"
        android:textSize="@dimen/size25" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonread"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/Layout_MonitorTitle"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="170dp"
            android:layout_height="160dp"
            android:layout_weight="1" >

            <AbsoluteLayout
                android:id="@+id/TextminT"
                android:layout_width="fill_parent"
                android:layout_height="158dp"
                android:layout_alignParentRight="true" >

                <ImageView
                    android:id="@+id/imagetemp"
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:src="@drawable/watch_tempture" />

                <TextView
                    android:id="@+id/textminT"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_x="28dp"
                    android:layout_y="69dp"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextMaxT"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_x="111dp"
                    android:layout_y="68dp"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextValueT"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="28dp"
                    android:layout_x="155dp"
                    android:layout_y="133dp"
                    android:hint="0"
                    android:textSize="@dimen/size20" />

                <TextView
                    android:id="@+id/textViewPathTitile"
                     android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                    android:text="@string/monitor_temp"
                    android:textSize="@dimen/size20" />

                <ImageView
                    android:id="@+id/ImageNeedleT"
                    android:layout_width="30dp"
                    android:layout_height="70dp"
                    android:layout_marginLeft="26dp"
                    android:layout_x="72dp"
                    android:layout_y="9dp"
                    android:src="@drawable/watch_needle" />
            </AbsoluteLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="170dp"
            android:layout_height="160dp"
            android:layout_weight="1" >

            <AbsoluteLayout
                android:id="@+id/ablayout2"
                android:layout_width="fill_parent"
                android:layout_height="160dp"
                android:layout_alignParentBottom="true" >

                <ImageView
                    android:id="@+id/imageHumit"
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:src="@drawable/watch_humitiy" />

                <TextView
                    android:id="@+id/TextMinh"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_x="31dp"
                    android:layout_y="67dp"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextMaxh"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_x="110dp"
                    android:layout_y="68dp"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/ImageNeedleH"
                    android:layout_width="30dp"
                    android:layout_height="70dp"
                    android:layout_marginLeft="26dp"
                    android:layout_marginTop="51dp"
                    android:layout_x="70dp"
                    android:layout_y="8dp"
                    android:src="@drawable/watch_needle" />

                <TextView
                    android:id="@+id/TextValueH"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_x="157dp"
                    android:layout_y="135dp"
                    android:hint="0"
                    android:textSize="@dimen/size20" />

            </AbsoluteLayout>

            <TextView
                android:id="@+id/TextView04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:text="@string/monitor_humity"
                android:textSize="@dimen/size20" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="170dp"
            android:layout_height="160dp"
            android:layout_gravity="bottom"
            android:layout_weight="1" >

            <AbsoluteLayout
                android:id="@+id/ablayout3"
                android:layout_width="160dp"
                android:layout_height="160dp"
                android:layout_alignBottom="@+id/intervel"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true" >

                <ImageView
                    android:id="@+id/imageBrightness"
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:src="@drawable/watch_brightness" />

                <TextView
                    android:id="@+id/TextMinB"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_x="28dp"
                    android:layout_y="69dp"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextMaxB"
                    android:layout_width="30dp"
                    android:layout_height="20dp"
                    android:layout_x="102dp"
                    android:layout_y="69dp"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/ImageNeedleB"
                    android:layout_width="30dp"
                    android:layout_height="70dp"
                    android:layout_marginLeft="21dp"
                    android:layout_x="70dp"
                    android:layout_y="9dp"
                    android:src="@drawable/watch_needle" />

                <TextView
                    android:id="@+id/TextValueB"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_x="156dp"
                    android:layout_y="133dp"
                    android:hint="0"
                    android:textSize="@dimen/size20" />

            </AbsoluteLayout>

            <TextView
                android:id="@+id/intervel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:text="@string/monitor_bright"
                android:textSize="@dimen/size20" />
        </RelativeLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/Connectstateshow"
        android:layout_width="650dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/buttonread"
        android:layout_alignBottom="@+id/buttonread"
        android:layout_toRightOf="@+id/buttonread"
        android:gravity="right"
        android:text="@string/monitor_connect_state_show"
        android:textSize="15sp"
        android:visibility="invisible" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/Connectstateshow"
        android:layout_alignLeft="@+id/Connectstateshow"
        android:layout_alignTop="@+id/linearLayout1"
        android:layout_toLeftOf="@+id/Layout_Navigation"
        android:background="@anim/shape" >

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/state1"
                style="@style/main_tab_but_linear"
                android:layout_weight="1"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/ImageView05"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/green" />

                <TextView
                    android:id="@+id/textViewUpdateItemSel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/monitor_commonstate"
                    android:textSize="@dimen/size20" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/state2"
                style="@style/main_tab_but_linear"
                android:layout_weight="1"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/ImageView04"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/red" />

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/monitor_overtemperature"
                    android:textSize="@dimen/size20" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/state3"
                style="@style/main_tab_but_linear"
                android:layout_weight="1"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/ImageView01"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/yellowrect" />

                <TextView
                    android:id="@+id/TextView02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/monitor_superwet"
                    android:textSize="@dimen/size20" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/state4"
                style="@style/main_tab_but_linear"
                android:layout_weight="1"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/ImageView01"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/brown" />

                <TextView
                    android:id="@+id/TextView03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/monitor_bad_lamp"
                    android:textSize="@dimen/size20" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/state5"
                style="@style/main_tab_but_linear"
                android:layout_weight="1"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/ImageView02"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/blue" />

                <TextView
                    android:id="@+id/TextView05"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/monitor_exception"
                    android:textSize="@dimen/size20" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/state6"
                style="@style/main_tab_but_linear"
                android:layout_weight="1"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/TextView06"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/monitor_connect_state"
                    android:textSize="@dimen/size20" />
            </LinearLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                 >

                <ImageView
                    android:id="@+id/backupstate"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:src="@drawable/backupstate" />
                <ImageView
                    android:id="@+id/disconnectstate"
                    android:layout_width="50dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:src="@drawable/disconnectstate" />

                <ImageView
                    android:id="@+id/connectstate"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:src="@drawable/connectstate" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/linearLayout2"
            android:orientation="vertical" >

            
        </LinearLayout>

    </RelativeLayout>

</RelativeLayout>