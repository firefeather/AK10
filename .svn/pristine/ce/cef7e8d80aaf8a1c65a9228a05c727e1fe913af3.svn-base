<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CabinetManageActivity" >

    <TextView
        android:id="@+id/tv_SeptalLine"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/top_cabinetlib"
        android:background="#F0FFF0" />

    <RelativeLayout
        android:id="@+id/top_cabinetlib"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_alignParentLeft="true"
        android:background="@drawable/title_tool_bg"
        >

        <TextView
            android:id="@+id/tvMessageAuto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:text="@string/leddisplay_cabinet_manage"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size" />

        <TextView
            android:id="@+id/btncabinetOK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:gravity="center"
             android:layout_marginRight="20dp"
          android:layout_marginTop="25dp"
            android:drawableLeft="@drawable/selector_tool_back"
            android:text="@string/cabinet_back_text"
            android:textColor="@color/white" />
    </RelativeLayout>

    <ListView
        android:id="@+id/listview_cabinetlib"
        android:layout_width="match_parent"
        android:layout_height="650dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/top_cabinetlib"
        android:cacheColorHint="#00000000"
        android:listSelector="#00000000"
        android:textColor="@color/white" >
    </ListView>

</RelativeLayout>